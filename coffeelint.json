{
	"indentation": {
		"value": 1
	},
	"max_line_length": {
		"level": "ignore"
	},
	"newline_at_eof": {
		"level": "error",
		"module": "coffeelint-newline-at-eof",
		"behaviour": "require"
	},
	"no_empty_functions": {
		"level": "warn"
	},
	"no_interpolation_in_single_quotes": {
		"level": "warn"
	},
	"no_tabs": {
		"level": "ignore"
	},
	"no_unnecessary_double_quotes": {
		"level": "warn"
	},
	"no_stand_alone_at": {
		"level": "warn"
	},
	"space_operators": {
		"level": "warn"
	}
}
